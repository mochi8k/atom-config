      "find-and-replace": true
      "git-diff": true
      "highlight-selected": true
      "minimap-autohide": true
      selection: true
  "pretty-json": {}
  react: {}
  "tree-view": {}
  welcome:
    showOnStartup: false
  zentabs:
    maximumOpenedTabs: 6
".babel.regexp.source":
