"*":
  "atom-alignment": {}
  "atom-beautify":
    _analyticsUserId: "8aa94c83-6aa8-414a-9d13-cdd6e685af6e"
    general:
      _analyticsUserId: "5a5c2a63-55ba-415c-954f-438ce2717f46"
